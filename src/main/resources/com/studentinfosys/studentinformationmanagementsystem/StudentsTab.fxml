<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="StudentVBox" prefHeight="675.0" prefWidth="874.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.studentinfosys.studentinformationmanagementsystem.StudentsTabController">
   <children>
      <VBox alignment="CENTER" prefHeight="44.0" prefWidth="874.0">
         <children>
            <Label alignment="CENTER" prefHeight="35.0" prefWidth="874.0" text="STUDENTS" textAlignment="CENTER">
               <font>
                  <Font name="Calibri" size="28.0" />
               </font>
            </Label>
         </children>
      </VBox>

                  <TableView fx:id="studentTableView" prefHeight="483.0" prefWidth="696.0">
        <columns>
          <TableColumn fx:id="IDColumn" prefWidth="86.4000244140625" text="ID" />
          <TableColumn fx:id="NameColumn" prefWidth="225.60001373291016" text="NAME" />
            <TableColumn fx:id="AddressColumn" prefWidth="258.3999938964844" text="ADDRESS" />
            <TableColumn fx:id="GuardianColumn" prefWidth="188.79998779296875" text="GUARDIAN" />
            <TableColumn fx:id="PhoneColumn" prefWidth="116.0" text="PHONE NUMBER" />
        </columns>
         <VBox.margin>
            <Insets bottom="18.0" />
         </VBox.margin>
      </TableView>
      <HBox alignment="CENTER" prefHeight="66.0" prefWidth="696.0" spacing="50.0">
         <children>
            <Button fx:id="Addbutton" mnemonicParsing="false" onAction="#AddButtonClicked" prefHeight="66.0" prefWidth="144.0" text="ADD">
               <font>
                  <Font name="Calibri" size="20.0" />
               </font></Button>
            <Button fx:id="UpdateButton" mnemonicParsing="false" onAction="#UpdateButtonClicked" prefHeight="66.0" prefWidth="144.0" text="UPDATE">
               <font>
                  <Font name="Calibri" size="20.0" />
               </font>
            </Button>
            <Button fx:id="DeleteButton" mnemonicParsing="false" onAction="#DeleteButtonPressed" prefHeight="66.0" prefWidth="144.0" text="DELETE">
               <font>
                  <Font name="Calibri" size="20.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
   </children>

</VBox>
